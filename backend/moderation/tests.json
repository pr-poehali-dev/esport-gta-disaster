{
  "tests": [
    {
      "name": "Get registrations without auth",
      "method": "GET",
      "path": "/",
      "expectedStatus": 401,
      "expectedBody": {
        "error": "User ID required"
      }
    },
    {
      "name": "Access denied for non-admin",
      "method": "GET",
      "path": "/",
      "headers": {
        "X-User-Id": "999"
      },
      "expectedStatus": 403,
      "expectedBody": {
        "error": "Access denied. Admin or organizer role required"
      }
    }
  ]
}
