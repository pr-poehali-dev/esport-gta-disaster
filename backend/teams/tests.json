{
  "tests": [
    {
      "name": "Get team without auth",
      "method": "GET",
      "path": "/",
      "expectedStatus": 401,
      "expectedBody": {
        "error": "User ID required"
      }
    },
    {
      "name": "Get team roster",
      "method": "GET",
      "path": "/?action=roster",
      "headers": {
        "X-User-Id": "1"
      },
      "expectedStatus": 200
    }
  ]
}